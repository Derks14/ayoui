{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "semi": ["error", "always"],
        "camelcase": "off",
        "no-underscore-dangle": "off",
        "import/extensions": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
